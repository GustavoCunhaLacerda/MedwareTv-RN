{
  "expo": {
    "name": "MedwareTv",
    "slug": "MedwareTv",
    "version": "1.0.0",
    "orientation": "landscape",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "native",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foreground": "./assets/banner_medware.png"
      },
      "intentFilters": [
        {
          "action": "MAIN",
          "category": ["LEANBACK_LAUNCHER"]
        }
      ],
      "package": "com.medware.mdwaretv"
    },
    "extra": {
      "eas": {
        "projectId": "14352a2e-9a47-4d45-8ac7-3e67bf4c08b0"
      }
    }
  }
}
